{"code":"import { Button, DropdownKebab, ListViewIcon, ListViewItem, MenuItem, OverlayTrigger, Tooltip, } from 'patternfly-react';\r\nimport * as React from 'react';\r\nexport class ViewListItem extends React.Component {\r\n    constructor() {\r\n        super(...arguments);\r\n        this.handleEdit = () => {\r\n            if (this.props.viewName) {\r\n                this.props.onEdit(this.props.viewName);\r\n            }\r\n        };\r\n        this.handleDelete = () => {\r\n            if (this.props.viewName) {\r\n                this.props.onDelete(this.props.viewName);\r\n            }\r\n        };\r\n    }\r\n    render() {\r\n        return (React.createElement(ListViewItem, { actions: React.createElement(\"div\", { className: \"form-group\" },\r\n                React.createElement(OverlayTrigger, { overlay: this.getEditTooltip(), placement: \"top\" },\r\n                    React.createElement(Button, { bsStyle: \"default\", onClick: this.handleEdit }, this.props.i18nEdit)),\r\n                React.createElement(DropdownKebab, { id: `view-${this.props.viewName}-action-menu`, pullRight: true },\r\n                    React.createElement(OverlayTrigger, { overlay: this.getDeleteTooltip(), placement: \"left\" },\r\n                        React.createElement(MenuItem, { onClick: this.handleDelete }, \"Delete\")))), heading: this.props.viewName, description: this.props.viewDescription ? this.props.viewDescription : '', hideCloseIcon: true, leftContent: this.props.viewIcon ? (React.createElement(\"div\", { className: \"blank-slate-pf-icon\" },\r\n                React.createElement(\"img\", { src: this.props.viewIcon, alt: this.props.viewName, width: 46 }))) : (React.createElement(ListViewIcon, { name: 'cube' })), stacked: false }));\r\n    }\r\n    getDeleteTooltip() {\r\n        return (React.createElement(Tooltip, { id: \"deleteTip\" }, this.props.i18nDeleteTip\r\n            ? this.props.i18nDeleteTip\r\n            : this.props.i18nDelete));\r\n    }\r\n    getEditTooltip() {\r\n        return (React.createElement(Tooltip, { id: \"editTip\" }, this.props.i18nEditTip ? this.props.i18nEditTip : this.props.i18nEdit));\r\n    }\r\n}\r\n//# sourceMappingURL=ViewListItem.js.map","map":"{\"version\":3,\"file\":\"ViewListItem.js\",\"sourceRoot\":\"\",\"sources\":[\"../../../../../../../src/Data/Virtualizations/Views/ViewListItem.tsx\"],\"names\":[],\"mappings\":\"AAAA,OAAO,EACL,MAAM,EACN,aAAa,EACb,YAAY,EACZ,YAAY,EACZ,QAAQ,EACR,cAAc,EACd,OAAO,GACR,MAAM,kBAAkB,CAAC;AAC1B,OAAO,KAAK,KAAK,MAAM,OAAO,CAAC;AAc/B,MAAM,OAAO,YAAa,SAAQ,KAAK,CAAC,SAA6B;IAArE;;QAiEU,eAAU,GAAG,GAAG,EAAE;YACxB,IAAI,IAAI,CAAC,KAAK,CAAC,QAAQ,EAAE;gBACvB,IAAI,CAAC,KAAK,CAAC,MAAM,CAAC,IAAI,CAAC,KAAK,CAAC,QAAQ,CAAC,CAAC;aACxC;QACH,CAAC,CAAC;QAEM,iBAAY,GAAG,GAAG,EAAE;YAC1B,IAAI,IAAI,CAAC,KAAK,CAAC,QAAQ,EAAE;gBACvB,IAAI,CAAC,KAAK,CAAC,QAAQ,CAAC,IAAI,CAAC,KAAK,CAAC,QAAQ,CAAC,CAAC;aAC1C;QACH,CAAC,CAAC;IACJ,CAAC;IA3EQ,MAAM;QACX,OAAO,CACL,oBAAC,YAAY,IACX,OAAO,EACL,6BAAK,SAAS,EAAC,YAAY;gBACzB,oBAAC,cAAc,IAAC,OAAO,EAAE,IAAI,CAAC,cAAc,EAAE,EAAE,SAAS,EAAC,KAAK;oBAC7D,oBAAC,MAAM,IAAC,OAAO,EAAC,SAAS,EAAC,OAAO,EAAE,IAAI,CAAC,UAAU,IAC/C,IAAI,CAAC,KAAK,CAAC,QAAQ,CACb,CACM;gBACjB,oBAAC,aAAa,IACZ,EAAE,EAAE,QAAQ,IAAI,CAAC,KAAK,CAAC,QAAQ,cAAc,EAC7C,SAAS,EAAE,IAAI;oBAEf,oBAAC,cAAc,IACb,OAAO,EAAE,IAAI,CAAC,gBAAgB,EAAE,EAChC,SAAS,EAAC,MAAM;wBAEhB,oBAAC,QAAQ,IAAC,OAAO,EAAE,IAAI,CAAC,YAAY,aAAmB,CACxC,CACH,CACZ,EAER,OAAO,EAAE,IAAI,CAAC,KAAK,CAAC,QAAQ,EAC5B,WAAW,EACT,IAAI,CAAC,KAAK,CAAC,eAAe,CAAC,CAAC,CAAC,IAAI,CAAC,KAAK,CAAC,eAAe,CAAC,CAAC,CAAC,EAAE,EAE9D,aAAa,EAAE,IAAI,EACnB,WAAW,EACT,IAAI,CAAC,KAAK,CAAC,QAAQ,CAAC,CAAC,CAAC,CACpB,6BAAK,SAAS,EAAC,qBAAqB;gBAClC,6BACE,GAAG,EAAE,IAAI,CAAC,KAAK,CAAC,QAAQ,EACxB,GAAG,EAAE,IAAI,CAAC,KAAK,CAAC,QAAQ,EACxB,KAAK,EAAE,EAAE,GACT,CACE,CACP,CAAC,CAAC,CAAC,CACF,oBAAC,YAAY,IAAC,IAAI,EAAE,MAAM,GAAI,CAC/B,EAEH,OAAO,EAAE,KAAK,GACd,CACH,CAAC;IACJ,CAAC;IAEO,gBAAgB;QACtB,OAAO,CACL,oBAAC,OAAO,IAAC,EAAE,EAAC,WAAW,IACpB,IAAI,CAAC,KAAK,CAAC,aAAa;YACvB,CAAC